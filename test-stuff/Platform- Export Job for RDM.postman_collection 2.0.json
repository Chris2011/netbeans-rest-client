	},
	"item": [
		{
			"name": "Get Analyses",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var responseBody = pm.response.json();",
							"pm.collectionVariables.set('analysis_uri', (responseBody[0].uri));"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "apikey",
					"apikey": {
						"value": "{{API_KEY}}",
						"key": "Authorization"
					}
				},
				"method": "GET",
				"header": [],
				"url": {
					"raw": "https://{{HOST}}/platform/riskdata/v1/analyses?filter=analysisName=\"{{ANALYSIS_NAME}}\"",
					"protocol": "https",
					"host": [
						"{{HOST}}"
					],
					"path": [
						"platform",
						"riskdata",
						"v1",
						"analyses"
					],
					"query": [
						{
							"key": "filter",
							"value": "analysisName=\"{{ANALYSIS_NAME}}\""
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "Get Resource Groups",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var responseBody = pm.response.json();",
							"pm.collectionVariables.set('resource_group_id', (responseBody[0].resourceGroupId));"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "apikey",
					"apikey": {
						"value": "{{API_KEY}}",
						"key": "Authorization"
					}
				},
				"method": "GET",
				"header": [],
				"url": "https://{{HOST}}/platform/tenantdata/v1/entitlements/RI-RISKMODELER/resourcegroups"
			},
			"response": []
		},
		{
			"name": "Create and Run Export Job",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.collectionVariables.set('export_job_url', pm.response.headers.get('Location'));",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var uuid = require('uuid');",
							"pm.collectionVariables.set('random_request_id', uuid.v4());"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "apikey",
					"apikey": {
						"value": "{{API_KEY}}",
						"key": "Authorization"
					}
				},
				"method": "POST",
				"header": [
					{
						"key": "x-rms-resource-group-id",
						"value": "{{resource_group_id}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"exportType\": \"RDM\",\n  \"resourceType\": \"analyses\",\n  \"settings\": {\n    \"fileExtension\": \"BAK\",\n    \"sqlVersion\": \"2019\",\n    \"rdmName\": \"Test_RDM_Export\"\n  },\n  \"resourceUris\": [\n    \"{{analysis_uri}}\"\n  ]\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": "https://{{HOST}}/platform/export/v1/jobs"
			},
			"response": []
		},
		{
			"name": "Get Export Job",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "apikey",
					"apikey": {
						"value": "{{API_KEY}}",
						"key": "Authorization"
					}
				},
				"method": "GET",
				"header": [],
				"url": "{{export_job_url}}"
			},
			"response": []
		}
	],
